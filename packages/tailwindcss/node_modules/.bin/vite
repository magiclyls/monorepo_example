#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/bin/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/bin/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@5.4.14_@types+node@22.13.1_lightningcss@1.29.1_sass@1.84.0_terser@5.38.1/node_modules/vite/bin/vite.js" "$@"
fi
