#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules/vitepress/bin/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules/vitepress/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules/vitepress/bin/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules/vitepress/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.41.0_@types+node@24.9.1_axios@1.12.2_lightning_5f6fa988496afbb03c1f3426d7abc5a5/node_modules:/Users/admin/Desktop/projects/bc-game/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
