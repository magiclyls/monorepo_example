#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules/vitepress/bin/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules/vitepress/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules/vitepress/bin/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules/vitepress/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.1_@types+node@22.13.1_axios@1.7.9_lightning_1e64c14ead481fcc1e58842acd77ae49/node_modules:/Users/admin/Desktop/bc-game/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
